{
	"root": "apps/eventria",
	"sourceRoot": "apps/eventria",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"root": "apps/eventria",
				"outputPath": "dist/apps/eventria"
			},
			"configurations": {
				"production": {}
			}
		},
		"serve": {
			"executor": "@nrwl/next:server",
			"options": {
				"buildTarget": "eventria:build",
				"dev": true
			},
			"configurations": {
				"production": {
					"buildTarget": "eventria:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nrwl/next:export",
			"options": {
				"buildTarget": "eventria:build:production"
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/eventria"],
			"options": {
				"jestConfig": "apps/marvice/jest.config.js",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/nftify/**/*.{ts,tsx,js,jsx}"]
			}
		}
	},
	"tags": []
}
