{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"jsxImportSource": "@emotion/react",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"types": ["node", "jest"],
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"@hs-platforms/hs-core-ui": ["../../libs/hs-core-ui/src/index.ts"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "next-env.d.ts", "../../libs/hs-core-ui/src/index.ts", "src/pages/test/index.ㅓㄴ"],
	"exclude": ["node_modules"]
}
